// RUN: aie-opt --aie-create-flows --aie-find-flows %s | FileCheck %s
// CHECK: %[[T03:.*]] = AIE.tile(0, 3)
// CHECK: %[[T02:.*]] = AIE.tile(0, 2)
// CHECK: %[[T00:.*]] = AIE.tile(0, 0)
// CHECK: %[[T13:.*]] = AIE.tile(1, 3)
// CHECK: %[[T11:.*]] = AIE.tile(1, 1)
// CHECK: %[[T10:.*]] = AIE.tile(1, 0)
// CHECK: %[[T22:.*]] = AIE.tile(2, 2)
// CHECK: %[[T31:.*]] = AIE.tile(3, 1)
// CHECK: %[[T73:.*]] = AIE.tile(7, 3)
// CHECK: %[[T70:.*]] = AIE.tile(7, 0)
// CHECK: %[[T80:.*]] = AIE.tile(8, 0)
//
// CHECK: AIE.flow(%[[T03]], Core : 0, %[[T02]], Core : 1)
// CHECK: AIE.flow(%[[T03]], Core : 1, %[[T02]], Core : 0)
// CHECK: AIE.flow(%[[T03]], DMA : 0, %[[T10]], DMA : 0)
// CHECK: AIE.flow(%[[T03]], DMA : 1, %[[T70]], DMA : 1)
// CHECK: AIE.flow(%[[T02]], DMA : 0, %[[T80]], DMA : 0)
// CHECK: AIE.flow(%[[T13]], Core : 1, %[[T31]], Core : 1)
// CHECK: AIE.flow(%[[T22]], Core : 0, %[[T13]], Core : 0)
// CHECK: AIE.flow(%[[T22]], DMA : 0, %[[T00]], DMA : 0)
// CHECK: AIE.flow(%[[T31]], DMA : 0, %[[T00]], DMA : 1)
// CHECK: AIE.flow(%[[T31]], DMA : 1, %[[T10]], DMA : 1)
// CHECK: AIE.flow(%[[T73]], Core : 0, %[[T31]], Core : 0)
// CHECK: AIE.flow(%[[T73]], Core : 1, %[[T22]], Core : 1)
// CHECK: AIE.flow(%[[T73]], DMA : 0, %[[T80]], DMA : 1)
// CHECK: AIE.flow(%[[T73]], DMA : 1, %[[T70]], DMA : 0)

module {
%t03 = AIE.tile(0, 3)
%t02 = AIE.tile(0, 2)
%t00 = AIE.tile(0, 0)
%t13 = AIE.tile(1, 3)
%t11 = AIE.tile(1, 1)
%t10 = AIE.tile(1, 0)
%t22 = AIE.tile(2, 2)
%t31 = AIE.tile(3, 1)
%t73 = AIE.tile(7, 3)
%t70 = AIE.tile(7, 0)
%t80 = AIE.tile(8, 0)

AIE.flow(%t03, DMA : 0, %t10, DMA : 0)
AIE.flow(%t03, DMA : 1, %t70, DMA : 1)
AIE.flow(%t02, DMA : 0, %t80, DMA : 0)
AIE.flow(%t22, DMA : 0, %t00, DMA : 0)

AIE.flow(%t22, Core : 0, %t13, Core : 0)
AIE.flow(%t03, Core : 1, %t02, Core : 0)
AIE.flow(%t73, Core : 0, %t31, Core : 0)
AIE.flow(%t73, Core : 1, %t22, Core : 1)

AIE.flow(%t73, DMA : 0, %t80, DMA : 1)
AIE.flow(%t73, DMA : 1, %t70, DMA : 0)
AIE.flow(%t31, DMA : 0, %t00, DMA : 1)
AIE.flow(%t31, DMA : 1, %t10, DMA : 1)

AIE.flow(%t03, Core : 0, %t02, Core : 1)
AIE.flow(%t13, Core : 1, %t31, Core : 1)

}
